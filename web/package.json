  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-boost": "^0.1.14",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "react": "^16.4.2",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.4",
    "react-skycons": "^0.7.0",
