{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "watch": "cpx \"../shared/**/*.*\" ./src/shared/ -w",
    "start": "concurrently \"npm run watch\" \"react-scripts start\"",
    "build": "cpx \"../shared/**/*.js\" ./src/shared/ && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "cpx": "^1.5.0"
  }
}
